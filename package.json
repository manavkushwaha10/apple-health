{
  "name": "apple-health",
  "version": "0.1.0",
  "description": "Apple HealthKit bindings",
  "main": "source/index.ts",
  "types": "build/index.d.ts",
  "bin": {
    "apple-health": "cli/index.ts"
  },
  "exports": {
    "./app.plugin": "./app.plugin.js",
    "./app.plugin.js": "./app.plugin.js",
    "./package.json": "./package.json",

    ".": {
      "types": "./build/index.d.ts",
      "default": "./src/index.ts"
    },
    "./hooks": {
      "types": "./build/hooks/index.d.ts",
      "default": "./src/hooks/index.ts"
    },
    "./dev-tools": {
      "types": "./build/dev-tools/index.d.ts",
      "default": "./src/dev-tools/index.ts"
    },
    "./cli": {
      "types": "./build/cli/index.d.ts",
      "default": "./cli/index.ts"
    }
  },
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "build:plugin": "tsc -p plugin",
    "cli": "bun cli/index.ts"
  },
  "keywords": [
    "react-native",
    "expo",
    "apple-health",
    "AppleHealth",
    "healthkit"
  ],
  "repository": "https://github.com/EvanBacon/apple-health",
  "bugs": {
    "url": "https://github.com/EvanBacon/apple-health/issues"
  },
  "author": "Evan Bacon <baconbrix@gmail.com> (EvanBacon)",
  "license": "MIT",
  "homepage": "https://github.com/EvanBacon/apple-health#readme",
  "dependencies": {
    "@stricli/core": "^1.1.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "expo-module-scripts": "^5.0.8",
    "expo": "^54.0.27",
    "react-native": "0.81.5"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "expo": {
    "autolinking": {
      "nativeModulesDir": "."
    }
  }
}
